# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_TOKEN=your-supabase-token

# OAuth Callback URLs
NEXT_PUBLIC_OAUTH_CALLBACK_URL=https://your-project-id.supabase.co/auth/v1/callback
NEXT_PUBLIC_LOCAL_CALLBACK_URL=http://localhost:3000/auth/callback

# Site URL (for redirects)
NEXT_PUBLIC_SITE_URL=https://your-app-domain.vercel.app # http://localhost:3000 

# Password Reset URL
NEXT_PUBLIC_RESET_PASSWORD_URL=http://localhost:3000/reset-password

# Google Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
ID_CLIENT_GOOGLE=your-google-client-id.apps.googleusercontent.com
SECRETA=your-google-client-secret

# LinkedIn Configuration
LINKEDIN_CLIENT_ID=your-linkedin-client-id
LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret
LINKEDIN_Access_token=your-linkedin-access-token

# GitHub Configuration
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
CLIENT_ID_GITHUB=your-github-client-id
CLIENT_SECRET_GITHUB=your-github-client-secret
GITHUB_CALL_BACK=http://localhost:3000/api/auth/callback/github

# Google Maps API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# SheetDB Configuration
SHEETDB_API_URL=https://sheetdb.io/api/v1/your-sheet-id
SHEETDB_API_KEY=your-sheetdb-api-key
NEXT_PUBLIC_SHEETDB_API_URL=https://sheetdb.io/api/v1/your-sheet-id
NEXT_PUBLIC_SHEETDB_API_KEY=your-sheetdb-api-key

# OAuth Providers (for local development with Supabase CLI)
SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET=your-google-client-secret
SUPABASE_AUTH_EXTERNAL_LINKEDIN_SECRET=your-linkedin-client-secret
SUPABASE_AUTH_EXTERNAL_GITHUB_SECRET=your-github-client-secret

