# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# OAuth Callback URLs
NEXT_PUBLIC_OAUTH_CALLBACK_URL=https://your-project-ref.supabase.co/auth/v1/callback
NEXT_PUBLIC_LOCAL_CALLBACK_URL=http://localhost:3000/auth/callback

# Site URL (for redirects)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# OAuth Provider Credentials (for Supabase Dashboard)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
LINKEDIN_CLIENT_ID=your-linkedin-client-id
LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# OAuth Providers (for local development with Supabase CLI)
SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET=your-google-client-secret
SUPABASE_AUTH_EXTERNAL_LINKEDIN_SECRET=your-linkedin-client-secret
SUPABASE_AUTH_EXTERNAL_GITHUB_SECRET=your-github-client-secret
